<%= form_for :af, url: add_friend_path, autocomplete: :off, remote: true do |f| %>
	<%= f.text_field :first_name, placeholder: "First name", class: "add-friend-tfield-fname" %>
	<%= f.text_field :last_name, placeholder: "Last name", class: "add-friend-tfield-lname" %>
	<%= f.submit "Add Friend", class: "add-friend-button" %>
<% end %>