<%= form_for :session, url: login_path, remote: true do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
	<%= f.submit "Log in" %>
<% end %>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>