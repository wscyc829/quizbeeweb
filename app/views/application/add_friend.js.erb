<% if @friend %>
	$("#friendlist-list").html("<%= escape_javascript(render 'friends/friends') %>")
	$("#roomlist-list").html("<%= escape_javascript(render 'rooms/rooms') %>")
<% else %>
	<% flash[:danger] = "Friend doesn't exist."%>
<% end %>

$("#add-friend-form").html("<%= j render 'friends/add_friend' %>")